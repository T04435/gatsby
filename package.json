{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "tslint:fix": "tslint --format verbose --fix --project .",
    "lint:fix": "yarn run tslint:fix && yarn run format",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@storybook/addon-essentials": "^6.0.25",
    "@types/node": "^14.11.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "babel-preset-react-app": "^9.1.2",
    "gatsby": "^2.24.63",
    "gatsby-plugin-layout": "^1.3.11",
    "gatsby-plugin-manifest": "^2.4.30",
    "gatsby-plugin-offline": "^3.2.28",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.3.11",
    "gatsby-plugin-robots-txt": "^1.5.2",
    "gatsby-plugin-segment-js": "^3.6.0",
    "gatsby-plugin-sitemap": "^2.4.13",
    "gatsby-plugin-tslint": "^0.0.2",
    "gatsby-plugin-typescript": "^2.4.20",
    "gatsby-plugin-typescript-checker": "^1.1.1",
    "gatsby-source-prismic": "^3.2.1",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-a11y": "^6.0.25",
    "@storybook/addon-actions": "^6.0.25",
    "@storybook/addon-links": "^6.0.25",
    "@storybook/addons": "^6.0.25",
    "@storybook/react": "^6.0.25",
    "babel-loader": "^8.1.0",
    "css-modules-typescript-loader": "^4.0.1",
    "dotenv": "^8.2.0",
    "gatsby-plugin-scss-typescript": "^4.0.13",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
